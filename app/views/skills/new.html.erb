<%= form_for(@skill) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br/>
<% f.label :category %>
<% f.text_field :category_id %><br/>
<%= f.label :description %>
<%= f.text_field :description %><br/>
<%= f.label :abbreviation %>
<%= f.text_field :abbreviation %><br/>
 <%= f.submit "Add skill" %>
 <% end %>